{
  "name": "@kunalnagarco/react-component-library",
  "version": "3.4.142",
  "description": "A collection of reusable React Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/kunalnagarco/react-component-library.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "postbuild": "find dist -type d -name '__stories__' -exec rm -r {} + && find dist -type f -name '*.stories.*' -delete",
    "build:storybook": "build-storybook -o release",
    "lint": "eslint --fix src/ --ext .ts,.tsx --max-warnings 0",
    "start": "start-storybook -p 9009",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "test:dev": "jest --watch --runInBand",
    "test": "jest --runInBand --coverage"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix src/ --ext .ts,.tsx --max-warnings 0"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "highlight.js": "10.7.3",
    "immer": "9.0.2",
    "marked": "2.0.5",
    "mem": ">=4.0.0",
    "node-fetch": "2.6.1",
    "node-notifier": "9.0.1",
    "prismjs": "1.24.1",
    "yargs-parser": ">=18.1.2"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@brainhubeu/react-carousel": "2.0.4",
    "@kunalnagarco/eslint-config": "0.1.33",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.4",
    "@rollup/plugin-url": "6.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.3",
    "@semantic-release/npm": "7.1.3",
    "@semantic-release/release-notes-generator": "9.0.3",
    "@storybook/addon-a11y": "6.3.6",
    "@storybook/addon-actions": "6.3.6",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-links": "6.3.6",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.3.6",
    "@svgr/rollup": "5.5.0",
    "@types/brainhubeu__react-carousel": "2.0.3",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.9",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.12",
    "@typescript-eslint/parser": "4.29.0",
    "@zerollup/ts-transform-paths": "1.7.18",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-preset-react": "6.24.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-typescript-prettier": "4.2.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.1.2",
    "normalize.css": "8.0.1",
    "polished": "4.1.3",
    "postcss-import": "14.0.2",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "rollup": "2.56.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-postcss": "4.0.0",
    "rollup-plugin-typescript2": "0.30.0",
    "semantic-release": "17.4.4",
    "sort-package-json": "1.50.0",
    "standard-version": "9.3.1",
    "styled-components": "5.3.0",
    "ttypescript": "1.5.12",
    "typescript": "4.3.5",
    "yaml-validator": "3.0.1"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5"
  },
  "publishConfig": {
    "access": "public"
  }
}
